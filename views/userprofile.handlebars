<!--Container -->
<div class="container" style="margin:100px">
  <div class="row">
    <div class="col-md-3">
      <div class="container">
        <div class="row">
          <img src="/Users/anusha/developer/Bantr/public/stylesheets/kitten-small (1).png" class="img-thumbnail"
            alt="...">
        </div>
        <div class="row">

          <ul class="list-group" style="margin-top:100px;width:250px">
            {{#each friends}}
            <li class="list-group-item" id="friends-list">{{this.name}}<img src="{{this.avatar}}" class="img-thumbnail"
